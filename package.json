{
  "name": "kisi-client",
  "version": "4.0.0",
  "repository": "https://github.com/kisi-inc/client-js",
  "license": "MIT",
  "description": "A JavaScript client for the Kisi API",
  "main": "./lib/index.js",
  "scripts": {
    "build": "mkdir -p ./lib && babel src/index.js --out-file lib/index.js",
