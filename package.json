  },
  "dependencies": {
    "axios": "latest",
    "humps": "latest"
  },
  "devDependencies": {
    "axios-mock-adapter": "latest",
    "babel-plugin-transform-regenerator": "latest",
    "babel-polyfill": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-es2017": "latest",
    "chai": "latest",
    "mocha": "latest"
  },
  "engines": {
    "node": "6.6.0"
  }
}
